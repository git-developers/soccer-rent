{% spaceless %}
    <div class="box box-solid">
        {#<div class="box-header with-border">#}
            {#<h3 class="box-title">About Me</h3>#}
        {#</div>#}

        <div class="box-body">

            <strong>
                <i class="fa fa-book margin-r-5"></i> Nombre
            </strong>

            <p class="text-muted">
                {{ entity.name }}
            </p>

            <hr>

            <strong>
                <i class="fa fa-map-marker margin-r-5"></i> latitude
            </strong>

            <p class="text-muted">
                {{ entity.latitude }}
            </p>

            <hr>

            <strong>
                <i class="fa fa-map-marker margin-r-5"></i> longitude
            </strong>

            <p class="text-muted">
                {{ entity.longitude }}
            </p>

            {% if entity.pointOfSale is not null %}
                <hr>

                <strong>
                    <i class="fa fa-home margin-r-5"></i> Puntos de venta padre
                </strong>

                <table class="table table-bordered">
                    <tbody>
                        <tr>
                            <th style="width: 10px">#</th>
                            <th>Nombre</th>
                            <th>latitude</th>
                            <th>longitude</th>
                            <th >Progress</th> {#style="width: 40px"#}
                        </tr>
                        <tr>
                            <td>
                                <span class="badge bg-orange">
                                    {{ entity.pointOfSale.idIncrement|e }}
                                </span>
                            </td>
                            <td>{{ entity.pointOfSale.name|e }}</td>
                            <td>{{ entity.pointOfSale.latitude|e }}</td>
                            <td>{{ entity.pointOfSale.longitude|e }}</td>
                            <td>
                                <div class="progress progress-xs">
                                    <div class="progress-bar progress-bar-danger" style="width: 55%"></div>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            {% endif %}

            <hr>

            <strong>
                <i class="fa fa-pencil margin-r-5"></i> Skills
            </strong>

            <p>
                <span class="label label-danger">UI Design</span>
                <span class="label label-success">Coding</span>
                <span class="label label-info">Javascript</span>
                <span class="label label-warning">PHP</span>
                <span class="label label-primary">Node.js</span>
            </p>

            <hr>

            <strong>
                <i class="fa fa-file-text-o margin-r-5"></i> Notes
            </strong>

            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Etiam fermentum enim neque.</p>

        </div>
    </div>
{% endspaceless %}